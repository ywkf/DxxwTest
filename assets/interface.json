{
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "dxxw",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "task": [
        {
            "name": "启动",
            "entry": "Startup"
        },
        {
            "name": "签到",
            "entry": "Signin"
        },
        {
            "name": "允许通知积分领取",
            "entry": "GetPoints"
        },
        {
            "name": "浏览图文作品",
            "entry": "MergeSigninGetpoints",
            "option": [
                "DxhSwitch"
            ],
            "pipeline_override": {
                "BrowseImagetxtComplete": {
                    "next": "Back"
                }
            }
        },
        {
            "name": "浏览音视频作品",
            "entry": "MergeSigninGetpoints",
            "option": [
                "DxhSwitch"
            ],
            "pipeline_override": {
                "SwipeUpivl": {
                    "next": "ClickVideoPart"
                },
                "BroswseVideoBack": {
                    "next": "Back"
                }
            }
        },
        {
            "name": "看视频听广播",
            "entry": "WatchVideoradio"
        },
        {
            "name": "观看直播",
            "entry": "MergeSigninGetpoints",
            "option": [
                "DxhSwitch"
            ],
            "pipeline_override": {
                "SwipeUpivl": {
                    "next": "ClickLivePart"
                },
                "WatchLiveBack": {
                    "next": "Back"
                }
            }
        },
        {
            "name": "发布作品",
            "entry": "Publish",
            "option": [
                "选择发布内容"
            ]
        },
        {
            "name": "发布作品x2",
            "entry": "PublishX2",
            "option": [
                "PublishSwitch",
                "PublishSwitch2"
            ]
        },
        {
            "name": "删除发布内容",
            "entry": "DeletePost"
        },
        {
            "name": "签到和领取通知每日积分",
            "entry": "MergeSigninGetpoints"
        },
        {
            "name": "完成全部浏览和发布（选择已关注的大象号）",
            "entry": "MergeBrowsePublish",
            "option": [
                "DxhSwitch"
            ]
        },
        {
            "name": "一键完成（选择已关注的大象号）",
            "entry": "MergeSigninGetpoints",
            "option": [
                "DxhSwitch",
                "PublishSwitch"
            ],
            "pipeline_override": {
                "GettingPoints": {
                    "next": "MergeBrowsePublish"
                },
                "SwipeUPsg": {
                    "next": [
                        "GettingPoints",
                        "MergeBrowsePublish"
                    ]
                }
            }
        },
        {
            "name": "一键完成（跳过图文）",
            "entry": "MergeBrowsePublish",
            "option": [
                "DxhSwitch",
                "PublishSwitch"
            ],
            "pipeline_override": {
                "SwipeUpivl": {
                    "next": "ClickVideoPart"
                }
            }
        },
        {
            "name": "一键完成（不发布作品）",
            "entry": "MergeSigninGetpoints",
            "option": [
                "DxhSwitch"
            ],
            "pipeline_override": {
                "GettingPoints": {
                    "next": "MergeBrowsePublish"
                },
                "TVRadioComplete02": {
                    "next": "Back"
                },
                "TVRadioTimeout02": {
                    "next": "Back"
                }
            }
        },
        {
            "name": "返回个人页",
            "entry": "Back"
        }
    ],
