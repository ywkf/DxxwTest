{
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "dxxw",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "task": [
        {
            "name": "启动",
            "entry": "Startup"
        },
        {
            "name": "签到",
            "entry": "Signin"
        },
        {
            "name": "允许通知积分领取",
            "entry": "GetPoints"
        },
        {
            "name": "浏览图文作品",
            "entry": "BrowseImagetxt_m"
        },
        {
            "name": "浏览音视频作品",
            "entry": "BrowseVedio_m"
        },
        {
            "name": "浏览图文和音视频作品",
            "entry": "BrowseImagetxt_mc"
        },
        {
            "name": "看视频听广播",
            "entry": "WatchVideoradio"
        },
        {
            "name": "观看直播",
            "entry": "WatchLive"
        },
        {
            "name": "发布作品",
            "entry": "Post"
        },
        {
            "name": "删除发布内容",
            "entry": "DeletePost"
        },
        {
            "name": "删除发布内容2（需接 删除发布 任务）",
            "entry": "PostMenu"
        },
        {
            "name": "返回个人页",
            "entry": "Back"
        },
        {
            "name": "任务",
            "entry": "Mission",
            "option": [
                "SigninSwitch",
                "GetPointsSwitch",
                "PostSwitch"
            ],
            "pipeline_override": {
                "Signin": {},
                "GetPoints": {},
                "BrowseImagetxt_m": {},
                "BrowseVedio_m": {},
                "WatchVideoradio": {},
                "WatchLive": {},
                "Post": {},
                "Post2": {},
                "DeletePost": {},
                "PostMenu": {}
            }
        }
    ],
    "option": {
        "SigninSwitch": {
            "cases": [
                {
                    "name": "启用",
                    "pipeline_override": {
                        "Signin": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "禁用",
                    "pipeline_override": {
                        "Signin": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "GetPointsSwitch": {
            "cases": [
                {
                    "name": "启用",
                    "pipeline_override": {
                        "GetPoints": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "禁用",
                    "pipeline_override": {
                        "GetPoints": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "PostSwitch": {
            "cases": [
                {
                    "name": "启用",
                    "pipeline_override": {
                        "Post": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "禁用",
                    "pipeline_override": {
                        "Post": {
                            "enabled": false
                        }
                    }
                }
            ]
        }
    }
}