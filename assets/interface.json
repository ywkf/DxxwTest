{
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "dxxw",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "task": [
        {
            "name": "启动",
            "entry": "Startup"
        },
        {
            "name": "签到",
            "entry": "Signin"
        },
        {
            "name": "允许通知积分领取",
            "entry": "GetPoints"
        },
        {
            "name": "浏览图文作品（个人号使用 需准备素材）",
            "entry": "BrowseImagetxtM"
        },
        {
            "name": "浏览音视频作品（个人号使用 需准备素材）",
            "entry": "BrowseVedioM"
        },
        {
            "name": "浏览图文和音视频作品",
            "entry": "BrowseImagetxtMC",
            "option": [
                "MyDxhSwitch"
            ]
        },
        {
            "name": "看视频听广播",
            "entry": "WatchVideoradio"
        },
        {
            "name": "观看直播",
            "entry": "WatchLive"
        },
        {
            "name": "发布作品",
            "entry": "Publish"
        },
        {
            "name": "发布作品x2（建议手动发布）",
            "entry": "PublishX2"
        },
        {
            "name": "删除发布内容x1（重复内容建议删除）",
            "entry": "DeletePost"
        },
        {
            "name": "签到和领取通知每日积分",
            "entry": "MergeSigninGetpoints"
        },
        {
            "name": "浏览图文、音视频和直播（选择已关注的大象号）",
            "entry": "MergeBrowsePublish",
            "option": [
                "DxhSwitch"
            ]
        },
        {
            "name": "返回个人页",
            "entry": "Back"
        },
        {
            "name": "多个任务以空格间隔",
            "entry": "Back"
        }
    ],
    "option": {
        "MyDxhSwitch": {
            "cases": [
                {
                    "name": "个人账号（已准备素材）",
                    "pipeline_override": {
                        "点击关注列表mc": {
                            "enabled": false
                        },
                        "BrowseImagetxtMC1": {
                            "next": "点击内容c"
                        }
                    }
                },
                {
                    "name": "关注账号（默认选择关注列表第一个）",
                    "pipeline_override": {
                        "点击关注列表mc": {
                            "enabled": true
                        },
                        "BrowseImagetxtMC1": {
                            "next": "点击关注列表mc"
                        }
                    }
                }
            ]
        },
        "DxhSwitch": {
            "cases": [
                {
                    "name": "河南信息广播",
                    "pipeline_override": {
                        "SelectDxh": {
                            "expected": "河南信息广播"
                        }
                    }
                },
                {
                    "name": "河南新闻广播",
                    "pipeline_override": {
                        "SelectDxh": {
                            "expected": "河南新闻广播"
                        }
                    }
                },
                {
                    "name": "大象新闻",
                    "pipeline_override": {
                        "SelectDxh": {
                            "expected": "大象新闻"
                        }
                    }
                },
                {
                    "name": "大象财富",
                    "pipeline_override": {
                        "SelectDxh": {
                            "expected": "大象财富"
                        }
                    }
                },
                {
                    "name": "都市频道",
                    "pipeline_override": {
                        "SelectDxh": {
                            "expected": "都市频道"
                        }
                    }
                },
                {
                    "name": "用户_1442826164161581057",
                    "pipeline_override": {
                        "SelectDxh": {
                            "expected": "用户_14428"
                        }
                    }
                },
                {
                    "name": "Dxh",
                    "pipeline_override": {
                        "SelectDxh": {
                            "expected": [
                                "用户_14428",
                                "河南信息广播",
                                "河南新闻广播",
                                "大象新闻",
                                "大象财富",
                                "都市报道"
                            ]
                        }
                    }
                }
            ]
        }
    }
}