{
    "name": "DxxwTest",
    "version": "0.0.91",
    "url": "https://github.com/ywkf/DxxwTest",
    "custom_title": "Dxxw",
    "default_controller": "adb",
    "lock_controller": false,
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "dxxw",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "task": [
        {
            "name": "启动",
            "entry": "Startup"
        },
        {
            "name": "重启",
            "entry": "Restart",
            "doc": "闪退重启，自定义添加"
        },
        {
            "name": "检测输入法",
            "entry": "InputmethodCheck00",
            "doc": "启用第三方输入法"
        },
        {
            "name": "延时/零点",
            "entry": "Sleep",
            "doc": "等待至零点或自定义时间",
            "option": [
                "SleepTime"
            ]
        },
        {
            "name": "签到",
            "entry": "Signin"
        },
        {
            "name": "允许通知积分领取",
            "entry": "GetPoints"
        },
        {
            "name": "浏览图文作品",
            "entry": "MergeBrowsePublish",
            "option": [
                "DxhSwitch"
            ],
            "pipeline_override": {
                "BrowseImagetxtComplete": {
                    "next": "Back"
                }
            }
        },
        {
            "name": "浏览音视频作品",
            "entry": "MergeBrowsePublish",
            "option": [
                "DxhSwitch"
            ],
            "pipeline_override": {
                "SwipeUpivl": {
                    "next": "ClickVideoPart"
                },
                "BroswseVideoBack": {
                    "next": "Back"
                }
            }
        },
        {
            "name": "看电视听广播",
            "entry": "WatchVideoradio"
        },
        {
            "name": "观看直播",
            "entry": "MergeBrowsePublish",
            "option": [
                "DxhSwitch"
            ],
            "pipeline_override": {
                "SwipeUpivl": {
                    "next": "ClickLivePart"
                },
                "LiveTimeout02": {
                    "next": "Back"
                }
            }
        },
        {
            "name": "发布作品",
            "entry": "Publish",
            "option": [
                "选择发布内容"
            ]
        },
        {
            "name": "发布作品x2",
            "entry": "PublishX2",
            "option": [
                "PublishSwitch",
                "PublishSwitch2"
            ]
        },
        {
            "name": "删除发布内容",
            "entry": "DeletePost"
        },
        {
            "name": "签到和领取通知每日积分",
            "entry": "MergeSigninGetpoints"
        },
        {
            "name": "完成全部浏览和发布（选择已关注的大象号）",
            "entry": "MergeBrowsePublish",
            "option": [
                "DxhSwitch"
            ]
        },
        {
            "name": "一键完成（选择已关注的大象号）",
            "entry": "InputmethodCheck00",
            "doc": "一键完成，选择大象号、发布作品类型、是否输入法检测",
            "option": [
                "DxhSwitch",
                "PublishSwitch",
                "输入法检测"
            ],
            "pipeline_override": {
                "SelectInputmethod": {
                    "next": "MergeSigninGetpoints"
                },
                "GettingPoints": {
                    "next": "MergeBrowsePublish"
                },
                "SwipeUPsg": {
                    "next": [
                        "GettingPoints",
                        "MergeBrowsePublish"
                    ]
                }
            }
        },
        {
            "name": "一键完成（跳过图文）",
            "entry": "MergeBrowsePublish",
            "option": [
                "DxhSwitch",
                "PublishSwitch"
            ],
            "pipeline_override": {
                "SwipeUpivl": {
                    "next": "ClickVideoPart"
                }
            }
        },
        {
            "name": "一键完成（不发布作品）",
            "entry": "InputmethodCheck00",
            "option": [
                "DxhSwitch",
                "输入法检测"
            ],
            "pipeline_override": {
                "SelectInputmethod": {
                    "next": "MergeSigninGetpoints"
                },
                "GettingPoints": {
                    "next": "MergeBrowsePublish"
                },
                "TVRadioComplete02": {
                    "next": "Back"
                },
                "TVRadioTimeout02": {
                    "next": "Back"
                }
            }
        },
        {
            "name": "任务完成检测",
            "entry": "TaskCheck",
            "doc": "不检测发布任务，放在流程最后",
            "pipeline_override": {
                "BrowseImagetxtComplete": {
                    "next": "TaskCheck"
                },
                "BroswseVideoBack": {
                    "next": "TaskCheck"
                },
                "LiveTimeout02": {
                    "next": "TaskCheck"
                },
                "完成看电视听广播2": {
                    "next": "TaskCheck"
                },
                "看电视听广播超时2": {
                    "next": "TaskCheck"
                }
            }
        },
        {
            "name": "返回个人页",
            "entry": "Back"
        }
    ],
